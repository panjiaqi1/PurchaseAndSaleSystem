<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.ExtendedFieldMapper">
    <select id="findAll" resultType="com.example.demo.entity.ExtendedField">
        select * from unit
    </select>

    <insert id="save" parameterType="com.example.demo.entity.ExtendedField">
        insert into unit (name)
        values (#{name,jdbcType=VARCHAR})
    </insert>

    <delete id="delete" parameterType="java.lang.Long">
        delete from unit where id= #{id,jdbcType=INTEGER}
    </delete>
</mapper>